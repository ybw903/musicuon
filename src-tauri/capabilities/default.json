{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main", "audio-player"],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "core:window:allow-set-size",
    "dialog:allow-open",
    "dialog:default",
    "sql:default",
    "sql:allow-execute"
  ]
}
